ggplot(
data = oktoberfest,
mapping = aes(
x = dauer,
fill = as.factor(dauer) # Färbung
)
) +
geom_bar() +
theme_linedraw() +
labs(
title = "Oktoberfest 1985 - 2023",
subtitle = "Länge des Oktoberfestes",
caption = "Source: https://opendata.muenchen.de/dataset/oktoberfest/resource/e0f664cf-6dd9-4743-bd2b-81a8b18bd1d2, [15.11.2024]",
x = "Dauer (Tage)",
y = "Anzahl der Jahre mit gleicher Dauer",
fill = "Dauer (Tage)" # Legendenbeschriftung
) +
scale_y_continuous(
breaks = seq(0, 30, by = 4)  # Intervall für y Achse -> seq(Start, Ende, Unterteilun)
) +
theme(
plot.title = element_text(hjust = 0.5), # hjust = 0 (Text links), = 0,5 (Text mittig), = 1 (Text rechts)
plot.subtitle = element_text(hjust = 0.5),
plot.caption = element_text(hjust = 1)
)
